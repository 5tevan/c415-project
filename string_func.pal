program stringfuncs (input, outout);
type
  string = array [1..4] of char;
var
  str : string;
  a : integer;
  b : real;

  function f1 (int : integer; s : string; r : real) : integer;
  begin
    f1 := int + 1;
    if s < '3456' then
      b := r + 1
  end;
begin
  str := '1234';
  a := 0;
  b := 0.5;
  a := f1(a, '2345', b);
end.
