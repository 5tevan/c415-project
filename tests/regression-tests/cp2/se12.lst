	vs = '1234567890';
	vc = '1234567890';
	va = '1234567890';
	vi = '1234567890';

type
	str = array[1..10] of char;
	chr = array[0..9] of char;
	arr = array[1..10] of array[1..1] of char;
	int = array[1..10] of integer;

var
	s1,s2,s3,s4:    str;
	c1,c2,c3,c4,c5: chr;
	a1,a2,a3,a4,a5: arr;
	i1,i2,i3,i4,i5: int;

begin

	s1 := vs;
	s2 := '1234567890';
	s3[1] := '1';
	s3[10] := '0';
	s4 := s3;

	write(s4);


	c1 := vc; //ERROR, wrong type
##semantic:38: assignment type is incompatible
