##semantic:39: assignment type is incompatible
	c3[0] := '0';
	c3[9] := '9';
	c4 := c3;
	c5 := s1; //ERROR, wrong type
##semantic:43: assignment type is incompatible

	write(c4); //ERROR, not a string
##semantic:45: Argmument 1 of 'write' must be of type integer, real, char, or string


	a1 := va; //ERROR, wrong type
##semantic:48: assignment type is incompatible
