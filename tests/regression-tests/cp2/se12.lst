##semantic:49: assignment type is incompatible
	a3[1] := '1';
	a3[2][1] := '2';
	a3[10] := '0';
	a4 := a3;
	a5 := s2; //ERROR, wrong type
##semantic:54: assignment type is incompatible

	write(a4); //ERROR, not a string
##semantic:56: Argmument 1 of 'write' must be of type integer, real, char, or string


	i1 := vi; //ERROR, wrong type
##semantic:59: assignment type is incompatible
	i2 := '1234567890'; //ERROR, wrong type
##semantic:60: assignment type is incompatible
	i3[1] := 1;
	i3[10] := 0;
	i4 := i3;
	i5 := s3; //ERROR, wrong type
##semantic:64: assignment type is incompatible

	write(i4); //ERROR, not a string
##semantic:66: Argmument 1 of 'write' must be of type integer, real, char, or string
end.

